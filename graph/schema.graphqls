# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Item {
  id: ID!
  unitPrice: Int!
  amount: Int!
  totalPrice: Int!
}

type User {
  id: ID!
  name: String!
}

type Query {
  Items: [Item!]!
}

input buyItemInput {
  id: String!
}

type Mutation {
  buyIem(input: buyItemInput!): Item!
}